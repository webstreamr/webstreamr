      align-items: center;
      justify-content: center;
      width: 2rem;
      height: 2rem;
      border-radius: 9999px;
      background-color: rgba(255, 153, 0, 0.3);
      flex-shrink: 0;
    }
 
    /* Title styling */
    .uploader-notes-title {
      font-weight: 600;
      font-size: 1rem;
      color: hsl(var(--foreground));
    }
 
    /* Content styling */
    .uploader-notes-content {
      padding: 1rem;
      color: hsl(var(--muted-foreground));
      font-size: 0.9375rem;
      line-height: 1.5;
    }
 
    /* Highlight important text */
