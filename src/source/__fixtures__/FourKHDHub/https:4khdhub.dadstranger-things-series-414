<script>document.addEventListener('DOMContentLoaded', function () {
    // Function to handle header click logic
    function setupHeaderClick(headers, getId, getContentId) {
        let previousHeader = null;

        headers.forEach(header => {
            header.addEventListener('click', function () {
                const id = getId(header);
                const contentFile = document.getElementById(getContentId(id));

                if (previousHeader && previousHeader !== header) {
                    previousHeader.style.backgroundColor = '';
                }

                if (contentFile.classList.contains('hidden')) {
                    header.style.backgroundColor = 'hsl(213deg 69.03% 35.03% / 68%)';
                } else {
                    header.style.backgroundColor = '';
                }

                previousHeader = header;
            });
        });
    }

    // Setup for download headers
    const downloadHeaders = document.querySelectorAll('.download-header');
    setupHeaderClick(
        downloadHeaders,
        header => header.getAttribute('data-file-id').replace('file', ''),
        id => 'content-file' + id
    );

    // Setup for episode headers
    const episodeHeaders = document.querySelectorAll('.episode-header');
    setupHeaderClick(
        episodeHeaders,
        header => header.getAttribute('data-episode-id'),
        id => 'content-' + id
    );
});

</script>

