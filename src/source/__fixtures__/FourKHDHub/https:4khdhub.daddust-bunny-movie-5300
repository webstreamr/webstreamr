        <!-- Video Player -->
        <iframe id="videoPlayer" 
                class="video-player" 
                src="https://player.videasy.net/movie/1043197"
                width="100%" 
                height="100%" 
                allowfullscreen="" 
                title="Video Player - Dust Bunny"
                referrerpolicy="origin" 
                loading="eager"
                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture">
        </iframe>
    </div>
     <script>
// Get DOM elements
const playButton = document.getElementById('play-btn');
const playerContainer = document.getElementById('playerContainer');
const goBackButton = document.getElementById('goBackButton');
const videoSource = document.getElementById('videoSource');
const videoPlayer = document.getElementById('videoPlayer');
const loadingOverlay = document.getElementById('loadingOverlay');

const videoSources = {
    '4K': 'https://player.videasy.net/movie/',
    'Autoembed': 'https://player.autoembed.cc/embed/movie/',
};

const defaultVideoId = '1043197';

// Track loading states
let loadingStates = {
    '4K': 'not-loaded', // 'not-loaded', 'loading', 'loaded', 'error'
    'Autoembed': 'not-loaded'
};

function showLoading() {
    loadingOverlay.classList.remove('hidden');
    videoSource.classList.add('source-changing');
}

function hideLoading() {
    loadingOverlay.classList.add('hidden');
    videoSource.classList.remove('source-changing');
}

function preloadDefaultVideo() {
    const defaultSource = loadVideoSourcePreference();
    
    if (loadingStates[defaultSource] === 'not-loaded') {
        loadingStates[defaultSource] = 'loading';
        
        const videoUrl = videoSources[defaultSource] + defaultVideoId;
        
        // Set up load event listener
        videoPlayer.onload = function() {
            loadingStates[defaultSource] = 'loaded';
            console.log(`${defaultSource} source preloaded successfully`);
            
            // Hide loading if player is currently visible and this is the active source
            if (!playerContainer.classList.contains('hidden') && videoSource.value === defaultSource) {
                hideLoading();
            }
        };
        
        // Set up error event listener
        videoPlayer.onerror = function() {
            loadingStates[defaultSource] = 'error';
            console.log(`${defaultSource} source failed to load`);
            
            // Hide loading if player is currently visible and this is the active source
            if (!playerContainer.classList.contains('hidden') && videoSource.value === defaultSource) {
                hideLoading();
            }
        };
        
        videoPlayer.src = videoUrl;
    }
}

function loadVideoSource(source) {
    if (loadingStates[source] === 'not-loaded') {
        loadingStates[source] = 'loading';
        
        const videoUrl = videoSources[source] + defaultVideoId;
        
        videoPlayer.onload = function() {
            loadingStates[source] = 'loaded';
            
            // Hide loading if player is visible and this is the active source
            if (!playerContainer.classList.contains('hidden') && videoSource.value === source) {
                hideLoading();
            }
        };
        
        videoPlayer.onerror = function() {
            loadingStates[source] = 'error';
            
            // Hide loading if player is visible and this is the active source
            if (!playerContainer.classList.contains('hidden') && videoSource.value === source) {
                hideLoading();
            }
        };
        
        videoPlayer.src = videoUrl;
    }
}

function showPlayer() {
    document.body.style.overflow = 'hidden';
    playerContainer.classList.remove('hidden');
    
    const selectedSource = videoSource.value;
    const currentState = loadingStates[selectedSource];
    
    switch (currentState) {
        case 'loaded':
            // Already loaded, show instantly
            hideLoading();
            break;
            
        case 'loading':
            // Currently loading, show loading overlay
            showLoading();
            break;
            
        case 'not-loaded':
            // Need to start loading
            showLoading();
            loadVideoSource(selectedSource);
            break;
            
        case 'error':
            // Previous load failed, try again
            showLoading();
            loadingStates[selectedSource] = 'not-loaded';
            loadVideoSource(selectedSource);
            break;
    }
}

function hidePlayer() {
    document.body.style.overflow = 'auto';
    playerContainer.classList.add('hidden');
    hideLoading(); // Always hide loading when closing player
}

function changeVideoSource(source) {
    const currentState = loadingStates[source];
    
    switch (currentState) {
        case 'loaded':
            // Already loaded, switch instantly
            const videoUrl = videoSources[source] + defaultVideoId;
            videoPlayer.src = videoUrl;
            hideLoading();
            break;
            
        case 'loading':
            // Currently loading, show loading overlay
            showLoading();
            break;
            
        case 'not-loaded':
            // Need to start loading
            showLoading();
            loadVideoSource(source);
            break;
            
        case 'error':
            // Previous load failed, try again
            showLoading();
            loadingStates[source] = 'not-loaded';
            loadVideoSource(source);
            break;
    }
}

// Preload after page load
window.addEventListener('load', function() {
    setTimeout(preloadDefaultVideo, 2000);
});

function saveVideoSourcePreference(source) {
    localStorage.setItem('preferredVideoSource', source);
}

function loadVideoSourcePreference() {
    return localStorage.getItem('preferredVideoSource') || '4K';
}

// Event listeners
playButton.addEventListener('click', showPlayer);
goBackButton.addEventListener('click', hidePlayer);

videoSource.addEventListener('change', function() {
    const selectedSource = this.value;
    saveVideoSourcePreference(selectedSource);
    changeVideoSource(selectedSource);
});

document.addEventListener('keydown', function(e) {
    if (e.key === 'Escape' && !playerContainer.classList.contains('hidden')) {
        hidePlayer();
    }
});

document.addEventListener('DOMContentLoaded', function() {
    const savedSource = loadVideoSourcePreference();
    videoSource.value = savedSource;
});
</script>
  

  <!-- Back to top button -->
  <button id="back-to-top" class="fixed bottom-6 right-6 z-50 rounded-full shadow-md transition-opacity duration-300 opacity-0 pointer-events-none bg-primary text-primary-foreground p-2" aria-label="Back to top">
    <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
      <path d="m18 15-6-6-6 6" />
    </svg>
  </button>

  <!-- Trailer Modal -->
