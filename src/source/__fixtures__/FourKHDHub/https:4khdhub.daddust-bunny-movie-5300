<script>document.addEventListener('DOMContentLoaded', function () {
    // Select all the download headers
    const downloadHeaders = document.querySelectorAll('.download-header');

    // Keep track of the previously opened header
    let previousHeader = null;

    downloadHeaders.forEach(header => {
        // Add click event to each download header
        header.addEventListener('click', function () {
            // Get the file id from the 'data-file-id' attribute of the clicked header
            const fileId = header.getAttribute('data-file-id').replace('file', '');
            const contentFile = document.getElementById('content-file' + fileId);

            // If there is a previously opened header, remove its background color
            if (previousHeader && previousHeader !== header) {
                previousHeader.style.backgroundColor = '';  // Remove the background color from the previous header
            }

            // Apply background color logic
            if (contentFile.classList.contains('hidden')) {
                // Apply background color when content is hidden
                header.style.backgroundColor = 'hsl(213deg 69.03% 35.03% / 68%)';
            } else {
                // Reset background color when content is open
                header.style.backgroundColor = '';
            }

            // Update the previous header to the current one
            previousHeader = header;
        });
    });
});
</script>
