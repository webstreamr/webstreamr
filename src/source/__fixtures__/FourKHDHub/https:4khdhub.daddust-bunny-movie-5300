  <!-- Fullscreen Player Container -->
    <div id="playerContainer" class="player-container hidden">
        <!-- Loading overlay -->
        <div id="loadingOverlay" class="loading-overlay hidden">
            <div class="loading-spinner"></div>
            <p class="loading-text">Loading video source...</p>
        </div>

        <!-- Top Controls -->
        <div class="controls">
            <!-- Go Back Button -->
            <button id="goBackButton" class="control-btn">
                <div class="arrow-left"></div>
            </button>
            
            <!-- Video Source Select -->
            <select id="videoSource" aria-label="Select video source" class="video-select">
                <option value="4K">4K</option>
                <option value="Autoembed">Autoembed</option>
               
            </select>
            
            <!-- Next Button (Hidden by default) -->
            <button id="nextButton" class="control-btn hidden">
                <div class="arrow-right"></div>
            </button>
